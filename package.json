{
	"name": "remotion-template",
	"version": "1.0.0",
	"description": "My Remotion video",
	"scripts": {
		"start": "remotion preview src/index.jsx",
		"build": "set NODE_OPTIONS=\"--max-old-space-size=14336\" && remotion render src/index.jsx Main out.mp4 --concurrency=12 --log=verbose",
		"upgrade": "remotion upgrade",
		"server": "ts-node server.tsx",
		"test": "eslint src --ext ts,tsx,js,jsx && tsc"
	},
	"repository": {},
	"license": "UNLICENSED",
	"dependencies": {
		"@google-cloud/text-to-speech": "^3.2.1",
		"@remotion/bundler": "^2.1.5",
		"@remotion/cli": "^2.1.5",
		"@remotion/eslint-config": "^2.1.5",
		"@remotion/renderer": "^2.1.5",
		"@types/express": "^4.17.9",
		"@types/react": "^17.0.0",
		"eslint": "^7.15.0",
		"express": "^4.17.1",
		"prettier": "^2.2.1",
		"prettier-plugin-organize-imports": "^1.1.1",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"remotion": "^2.1.5",
		"ts-node": "^9.1.1",
		"typescript": "^4.2.3"
	},
	"devDependencies": {
		"autoprefixer": "^10.2.5",
		"postcss": "^8.3.0",
		"tailwindcss": "^2.1.2"
	}
}
